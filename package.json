{
  "name": "obsidian-kindle-book-info",
  "version": "1.0.0",
  "description": "Obsidian plugin to fetch Kindle book information from share URLs and create notes",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "kindle",
    "amazon",
    "book"
  ],
  "author": "mizuki-momose",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mizuki-momose/obsidian-kindle-book-info.git"
  },
  "homepage": "https://github.com/mizuki-momose/obsidian-kindle-book-info",
  "bugs": {
    "url": "https://github.com/mizuki-momose/obsidian-kindle-book-info/issues"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.8",
    "obsidian": "latest",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12"
  },
  "volta": {
    "node": "24.11.0",
    "pnpm": "8.12.0"
  }
}
